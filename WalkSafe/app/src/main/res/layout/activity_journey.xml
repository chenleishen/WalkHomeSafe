<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.deltahacks.walksafe.JourneyActivity">



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="A journey of a thousand miles begins with the first step"
        android:id="@+id/textView"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/imageView2"/>


    <ImageButton
        android:contentDescription="@string/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/helpicon"
        android:src="@drawable/helpicon"
        android:onClick="helpMe"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:contentDescription="@string/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/homeButton"
        android:src="@drawable/homeiconsmall"
        android:onClick="homeSafe"
        android:layout_below="@+id/helpicon"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/header"

        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
